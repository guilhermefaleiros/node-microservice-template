{
  "name": "node-setup",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Guilherme Faleiros <guilhermefaleiros2000@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start": "ENVIRONMENT=PRODUCTION && node -r ./dist/main/tracing.js ./dist/main/index.js",
    "dev:telemetry": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/main/telemetry.ts",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/main/index.ts",
    "test": "jest",
    "format": "prettier --write \"src/**/*.{js,jsx}\""
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.2",
    "@babel/node": "^7.16.8",
    "@babel/plugin-transform-modules-commonjs": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/express": "^4.17.13",
    "@types/ioredis": "^4.28.8",
    "@types/jest": "^27.4.0",
    "@types/node-cron": "^3.0.1",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@entropy/winston-elasticsearch-apm": "^1.0.8",
    "@opentelemetry/api": "^1.0.4",
    "@opentelemetry/exporter-jaeger": "^1.0.1",
    "@opentelemetry/instrumentation": "^0.27.0",
    "@opentelemetry/instrumentation-express": "^0.27.0",
    "@opentelemetry/instrumentation-http": "^0.27.0",
    "@opentelemetry/instrumentation-pg": "^0.28.0",
    "@opentelemetry/resources": "^1.0.1",
    "@opentelemetry/sdk-trace-base": "^1.0.1",
    "@opentelemetry/sdk-trace-node": "^1.0.1",
    "@opentelemetry/semantic-conventions": "^1.0.1",
    "@prisma/client": "3.9.2",
    "axios": "^0.26.0",
    "dotenv": "^16.0.0",
    "elastic-apm-node": "^3.29.0",
    "express": "^4.17.2",
    "express-async-errors": "^3.1.1",
    "ioredis": "^4.28.5",
    "kafkajs": "^1.16.0",
    "node-cron": "^3.0.0",
    "opentelemetry-instrumentation-kafkajs": "^0.27.0",
    "prisma": "^3.9.2",
    "uuid": "^8.3.2",
    "winston": "^3.5.1",
    "winston-elasticsearch": "^0.16.1",
    "winston-logstash-transport": "^2.0.0"
  }
